	<!--===== Survival Meal =====-->
	<RecipeDef Name="RJW_Gastronomy_SurvivalMealBase" ParentName="RJW_Gastronomy_Base_Recipe" Abstract="True">
		<researchPrerequisite>PackagedSurvivalMeal</researchPrerequisite>
		<skillRequirements>
			<Cooking>8</Cooking>
		</skillRequirements>
	</RecipeDef>

	<RecipeDef ParentName="RJW_Gastronomy_SurvivalMealBase">
		<defName>RJW_Gastronomy_SurvivalMealSingle</defName>
		<description>Prepare a packaged survival meal from a combination of various erotic ingredients. Some of the ingredients are wasted.</description>
		<jobString>Cooking erotic survival meal</jobString>
		<label>cook erotic packaged survival meal</label>
		<displayPriority>15</displayPriority>
		<ingredients>
		<li>
			<filter>
				<categories>
					<li>RJW_Gastronomy_Humilk</li>
					<li>MeatRaw</li>
				</categories>
			</filter>
			<count>0.30</count>
		</li>
		<li>
			<filter>
				<categories>
					<li>RJW_Gastronomy_FoodRaw</li>
				</categories>
				<disallowedCategories>
					<li>MeatRaw</li>
					<li>RJW_Gastronomy_Humilk</li>
				</disallowedCategories>
			</filter>
			<count>0.30</count>
		</li>
		</ingredients>
		<workAmount>500</workAmount>
		<products>
			<RJW_Gastronomy_MealSurvivalPack>1</RJW_Gastronomy_MealSurvivalPack>
		</products>
	</RecipeDef>

