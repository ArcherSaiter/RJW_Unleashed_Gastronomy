	<!-- ===== Gourmet ===== -->
	<RecipeDef Name="RJW_Gastronomy_Base_Recipe_Gourmet" ParentName="RJW_Gastronomy_Base_Recipe" Abstract="True">
		<!-- VCE custom ingredientValueGetterClass -->
		<!-- If non ingestable or condiment, use flat amount -->
		<!--<ingredientValueGetterClass MayRequire="vanillaexpanded.vcooke">VanillaCookingExpanded.IngredientValueGetter_NutritionWithExtraIngredient</ingredientValueGetterClass>-->
		<description>Cook a culinary work of art, elevating the taste of each ingredient to the peak, with refined, even elaborate preparation and presentation of an aesthetic and balanced meal. Most of the ingredients are discarded, leaving only the best of the best.</description>
		<displayPriority>13</displayPriority>
		<skillRequirements>
			<Cooking>10</Cooking>
		</skillRequirements>
		<fixedIngredientFilter>
			<categories>
			<li MayRequire="vanillaexpanded.vcooke">VCE_Condiments</li>
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
			<li MayRequire="vanillaexpanded.vcooke">VCE_Condiments</li>
			</categories>
		</defaultIngredientFilter>
	</RecipeDef>

