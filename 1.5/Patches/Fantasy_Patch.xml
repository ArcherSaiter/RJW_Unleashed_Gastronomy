<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Fantasy Races</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
				<!-- Similar Xenotype Removal -->
                <li Class="PatchOperationRemove">
                    <xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Catgirl[text()="0.2"]</xpath>
                </li>
				<li Class="PatchOperationRemove">
                    <xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Succubus[text()="0.05"]</xpath>
                </li>
                <!-- Value Adjustment -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Foxgirl</xpath>
					<value>
						<EFR_Foxgirl>0.25</EFR_Foxgirl>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Orc</xpath>
					<value>
						<EFR_Orc>0.25</EFR_Orc>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Harpy</xpath>
					<value>
						<EFR_Harpy>0.15</EFR_Harpy>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Arachne</xpath>
					<value>
						<EFR_Arachne>0.25</EFR_Arachne>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[@Name="EFR_FantasyTribeBase"]/xenotypeSet/xenotypeChances/EFR_Slimegirl</xpath>
					<value>
						<EFR_Slimegirl>0.25</EFR_Slimegirl>
					</value>
				</li>
            </operations>
        </match>
    </Operation>
</Patch>