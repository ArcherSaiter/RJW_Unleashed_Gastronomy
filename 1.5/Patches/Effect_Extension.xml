				<modExtensions>
					<li Class="RJW_Unleashed_Framework.SourceEffectLinkEx">
						<sourceName>RJWReProduce_RawToxicle</sourceName>
						<effectHediffDef>ToxicBuildup</effectHediffDef>
						<severityMultiplier>-0.15</severityMultiplier>
