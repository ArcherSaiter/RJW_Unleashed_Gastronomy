	    <harvestWork>300</harvestWork>
	    <harvestTag>Standard</harvestTag>
	    <harvestAfterGrowth>0.35</harvestAfterGrowth>
	    <harvestedThingDef>RawSleepBerries</harvestedThingDef>
	    <wildClusterRadius>4</wildClusterRadius>
	    <wildClusterWeight>200</wildClusterWeight>
	    <visualSizeRange>0.9~1.6</visualSizeRange>
	    <topWindExposure>0.25</topWindExposure>
